# ModelScope-only mode configuration
# Copy this to .env or use with environment variable override

# GitHub tokens (required)
GITHUB_TOKENS=your_github_tokens_here

# Data path
DATA_PATH=./data

# ModelScope specific settings
TARGET_BASE_URLS=https://api-inference.modelscope.cn/v1/,api-inference.modelscope.cn
MODELSCOPE_EXTRACT_ONLY=true
MS_USE_LOOSE_PATTERN=false
MS_PROXIMITY_CHARS=0

# Disable other extractors
OPENROUTER_BASE_URLS=
OPENROUTER_EXTRACT_ONLY=false

# Use ModelScope specific queries
QUERIES_FILE=modelscope_queries.txt

# Optional: Proxy settings
# PROXY=http://localhost:1080