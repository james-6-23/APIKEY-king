# OpenRouter-only mode configuration (with validation)
# Copy this to .env or use with environment variable override

# GitHub tokens (required)
GITHUB_TOKENS=your_github_tokens_here

# Data path
DATA_PATH=./data

# OpenRouter specific settings
OPENROUTER_BASE_URLS=https://openrouter.ai/api/v1,openrouter.ai
OPENROUTER_EXTRACT_ONLY=false  # Enable validation
OPENROUTER_USE_LOOSE_PATTERN=false
OPENROUTER_PROXIMITY_CHARS=0

# OpenRouter validation settings
OPENROUTER_VALIDATION_ENABLED=true
OPENROUTER_TEST_MODEL=deepseek/deepseek-chat-v3.1:free
OPENROUTER_TIMEOUT=30.0

# Disable other extractors by clearing their base URLs
TARGET_BASE_URLS=
MODELSCOPE_EXTRACT_ONLY=false

# Use OpenRouter specific queries
QUERIES_FILE=openrouter_queries.txt

# Optional: Proxy settings
# PROXY=http://localhost:1080